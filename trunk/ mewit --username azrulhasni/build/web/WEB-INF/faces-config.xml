<!-- =========== FULL CONFIGURATION FILE ================================== -->
<faces-config version="1.2" 
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <application>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>en</supported-locale>
            <supported-locale>ja</supported-locale>
            <supported-locale>zh_CN</supported-locale>
            <supported-locale>pt_BR</supported-locale>
        </locale-config>
    </application>
    <component>
        <description>The calendar component. </description>
        <display-name>The calendar component. </display-name>
        <icon>
            <small-icon>uicalendar-small.jpg</small-icon>
            <large-icon>uicalendar-large.jpg</large-icon>
        </icon>
        <component-type>Calendar</component-type>
        <component-class>net.metacube.components.calendar.UICalendar</component-class>
        <property>
            <description>The current date.</description>
            <property-name>currentDate</property-name>
            <property-class>java.util.Date</property-class>
        </property>
    </component>
    <render-kit>
        <renderer>
            <description>Renderer for a net.metacube.components.calendar.UICalendar.</description>
            <component-family>Calendar</component-family>
            <renderer-type>CalendarRenderer</renderer-type>
            <renderer-class>net.metacube.components.calendar.CalendarRenderer
            </renderer-class>
        </renderer>
    </render-kit>
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PgLogin</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgLogin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/PgLogin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>successful</from-outcome>
            <to-view-id>/PgMenu.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>quickAccess</from-outcome>
            <to-view-id>/PgReceivedItem.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failed</from-outcome>
            <to-view-id>/PgLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>PgReceivedItem</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgReceivedItem</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
   
    <managed-bean>
        <managed-bean-name>PgSentItem</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgSentItem</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
   
    <managed-bean>
        <managed-bean-name>PgEditProfile</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgEditProfile</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
   
    <managed-bean>
        <managed-bean-name>PgManagePersonalLists</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgManagePersonalLists</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>PgMenu</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgMenu</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>gotoNewItem</from-outcome>
            <to-view-id>/PgNewItem.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/PgLogin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoManagePersonalLists</from-outcome>
            <to-view-id>/PgManagePersonalLists.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoEditProfile</from-outcome>
            <to-view-id>/PgEditProfile.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoSearch</from-outcome>
            <to-view-id>/PgSearch.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoManagedSavedSearch</from-outcome>
            <to-view-id>/PgManageSavedSearch.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoMenu</from-outcome>
            <to-view-id>/PgMenu.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoReceivedItem</from-outcome>
            <to-view-id>/PgReceivedItem.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoSentItem</from-outcome>
            <to-view-id>/PgSentItem.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoFileRepository</from-outcome>
            <to-view-id>/PgFileRepository.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoFileFromItems</from-outcome>
            <to-view-id>/PgFileFromItems.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>gotoUserInfo</from-outcome>
            <to-view-id>/PgUserInfo.jsp</to-view-id>
        </navigation-case>
        
         <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/PgLogin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>PgSupervisedItems</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgSupervisedItems</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
   
    <managed-bean>
        <managed-bean-name>PgManageBasket</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgManageBasket</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PgSearch</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgSearch</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PgManageSavedSearch</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgManageSavedSearch</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
   
  
    <managed-bean>
        <managed-bean-name>PgfTopMenu</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgfTopMenu</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PgTreeView</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgTreeView</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>PgFileRepository</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgFileRepository</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
   
    <managed-bean>
        <managed-bean-name>PgNewItem</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgNewItem</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>calMarkers</managed-bean-name>
        <managed-bean-class>net.metacube.components.calendar.CalendarMarkerCollections </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>calModel</managed-bean-name>
        <managed-bean-class>net.metacube.components.calendar.CalendarModel</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PgUserInfo</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgUserInfo</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PgFileFromItems</managed-bean-name>
        <managed-bean-class>org.azrul.epice.web.PgFileFromItems</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean> 
</faces-config>
